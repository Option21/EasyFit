<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="easyfit.easyfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_test_1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Exercices.ItemListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Exercices.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".Exercices.ItemListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Exercices.ItemListActivity" />
        </activity>
        <activity
            android:name=".ProgramList.ProgramListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProgramList.ProgramDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".Exercices.ItemListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Exercices.ItemListActivity" />
        </activity>

        <activity
            android:name=".BaseDrawerActivity"
            android:label="@string/title_activity_base_drawer"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity android:name=".CreationProfile"
            android:label="CreationProfile"></activity>
    </application>

</manifest>
